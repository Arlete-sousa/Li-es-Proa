programa
{
inteiro numero,resultado,contador

	
	funcao inicio()
	{
		escreva("Escrava o numero da tabuada desejada: ")
		leia(numero)

		limpa()

		para(contador = 1; contador <= 10; contador ++)
		{
			resultado = numero * contador
			escreva(numero, "X", contador, "=", resultado, "\n")
		}
		
	}
}

programa
{
	real numero
	
	funcao inicio()
	{
		escreva("O valor desejado: ")
		leia(numero)

		limpa()

		se(numero >0)
		escreva("positivo")
		senao
		
		programa
{
	inteiro n1, n2, n3
	
	funcao inicio()
	{
		escreva("Digite o primeiro nomero: ")
             leia(n1)
          escreva("Digite o segundo numero: ")
             leia(n2)
          escreva("Digite o terceiro numero: ")
             leia(n3)

          se (n1>=n2) se (n1>=n3)
             escreva("O numero maior é: ", n1)
          se (n2>=n1) se (n2>=n3)
             escreva("O numero maior é: ", n2)
          se (n3>=n1) se (n3>=n2)
             escreva("O numero maior é: ", n3)

         
          
		
	}
}
		escreva("negativo")



		
	}
}
