programa
{
inteiro numero,resultado,contador

	
	funcao inicio()
	{
		escreva("Escrava o numero da tabuada desejada: ")
		leia(numero)

		limpa()

		para(contador = 1; contador <= 10; contador ++)
		{
			resultado = numero * contador
			escreva(numero, "X", contador, "=", resultado, "\n")
		}
		
	}
}

programa
{
	real numero
	
	funcao inicio()
	{
		escreva("O valor desejado: ")
		leia(numero)

		limpa()

		se(numero >0)
		escreva("positivo")
		senao
		escreva("negativo")



		
	}
}
